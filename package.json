{
  "name": "krrish-co",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .astro",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky install",
    "predeploy": "bun run lint && bun run test && bun run build",
    "deploy": "astro deploy",
    "deploy:vercel": "vercel",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:gh-pages": "gh-pages -d dist",
    "deploy:staging": "vercel --staging",
    "postdeploy": "bun run test:e2e",
    "analyze": "astro build --analyze",
    "check:security": "bun audit",
    "check:updates": "bun outdated",
    "check:size": "bun run build && size-limit"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^3.1.9",
    "@astrojs/react": "^3.6.3",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.2",
    "@iconify-json/lucide": "^1.2.17",
    "@iconify-json/ph": "^1.2.1",
    "@iconify/json": "^2.2.277",
    "@keystatic/astro": "^5.0.3",
    "@keystatic/core": "^0.5.42",
    "@types/aos": "^3.0.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "aos": "^2.3.4",
    "astro": "^4.16.16",
    "astro-icon": "^1.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@testing-library/react": "^14.0.0",
    "@axe-core/playwright": "^4.8.5",
    "@vitest/coverage-v8": "^0.34.6",
    "gh-pages": "^6.1.1",
    "netlify-cli": "^17.0.0",
    "vercel": "^33.0.0",
    "@size-limit/preset-app": "^11.0.0",
    "size-limit": "^11.0.0",
    "husky": "^9.1.7"
  }
}
